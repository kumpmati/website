{
	"name": "website",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postbuild": "npx svelte-sitemap --domain https://matsku.dev"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.34",
		"@sveltejs/kit": "^1.0.0-next.312",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"contentful": "^9.1.18",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.45.1",
		"svelte": "^3.47.0",
		"svelte-check": "^2.2.6",
		"svelte-markdown": "^0.2.2",
		"svelte-preprocess": "^4.9.4",
		"svelte-sitemap": "^2.5.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"socket.io-client": "^4.4.1",
		"svelte-feather-icons": "^3.5.0",
		"svelte-intersection-observer": "^0.9.2"
	}
}
